            <element column="Ins_Clave" name="Ins_Clave" xsdType="xs:string" optional="true"/>
            <element column="Ins_Descri" name="Ins_Descri" xsdType="xs:string" optional="true"/>
        </result>
    </query>
    <query id="cuentaDestinoBIMActualizarSQL" useConfig="BIMDB">
